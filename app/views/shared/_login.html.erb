<% if !current_user.nil? %>
	<form action="<%= session_url(session) %>" method="POST">
		<button type="submit">Log Out!</button>   LOGGED IN AS: <%= current_user.username %>
		<input
		     name="authenticity_token"
		     type="hidden"
		     value="<%= form_authenticity_token %>">
		<input name="_method" type="hidden" value="DELETE">
	</form>

<% else %>
<form action="<%= new_session_url %>" method="GET"><button type="submit">Log In!</button></form>

<% end %>